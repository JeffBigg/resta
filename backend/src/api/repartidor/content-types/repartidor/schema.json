    "singularName": "repartidor",
    "pluralName": "repartidors",
    "displayName": "Repartidor"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "nombre": {
      "type": "string"
    },
    "telefono": {
      "type": "string"
    },
    "estado": {
