{
  "kind": "collectionType",
  "collectionName": "empleados",
  "info": {
    "singularName": "empleado",
    "pluralName": "empleados",
    "displayName": "Empleado"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "nombre": {
      "type": "string"
    },
    "apellido": {
      "type": "string"
    },
    "pin_code": {
      "type": "string"
    },
